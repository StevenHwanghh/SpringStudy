<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
 http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="user1" class="com.hh.User">
		<constructor-arg value="1"></constructor-arg>
		<constructor-arg value="Steven Huang"></constructor-arg>
		<constructor-arg value="Steven.hwanghh@gmail.com"></constructor-arg>
	</bean>

	<bean id="user2" class="com.hh.User">
		<constructor-arg value="2"></constructor-arg>
		<constructor-arg value="kevin Huang"></constructor-arg>
		<constructor-arg value="Kevin.hwanghh@gmail.com"></constructor-arg>
	</bean>
	
	<bean id="answer1" class="com.hh.Answer">
		<constructor-arg value="1"></constructor-arg>
		<constructor-arg
			value="Java is a Programming Language"></constructor-arg>
		<constructor-arg value="04/28/2018"></constructor-arg>
	</bean>

	<bean id="answer2" class="com.hh.Answer">
		<constructor-arg value="2"></constructor-arg>
		<constructor-arg value="Java is a Platform"></constructor-arg>
		<constructor-arg value="01/12/2003"></constructor-arg>
	</bean>

	<bean id="question" class="com.hh.Question">
		<constructor-arg value="111"></constructor-arg>
		<constructor-arg value="What is java?"></constructor-arg>
		<constructor-arg>
			<map>
				<entry key-ref="answer1" value-ref="user1" />
				<entry key-ref="answer2" value-ref="user2" />
			</map>
		</constructor-arg>

	</bean>


</beans>